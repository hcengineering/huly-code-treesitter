{
  "rust": {
    "extensions": ["rs"],
    "className": "org.treesitter.TreeSitterRust"
  },
  "zig": {
    "extensions": ["zig"],
    "className": "org.treesitter.TreeSitterZig"
  },
  "typescript": {
    "extensions": ["ts"],
    "className": "org.treesitter.TreeSitterTypescript"
  },
  "javascript": {
    "extensions": ["js", "jsx"],
    "className": "org.treesitter.TreeSitterJavascript"
  },
  "svelte": {
    "extensions": ["svelte"],
    "className": "org.treesitter.TreeSitterSvelte"
  },
  "astro": {
    "extensions": ["astro"],
    "className": "org.treesitter.TreeSitterAstro"
  },
  "css": {
    "extensions": ["css"],
    "className": "org.treesitter.TreeSitterCss"
  },
  "html": {
    "extensions": ["html", "htm"],
    "className": "org.treesitter.TreeSitterHtml"
  },
  "go": {
    "extensions": ["go"],
    "className": "org.treesitter.TreeSitterGo"
  },
  "scss": {
    "extensions": ["scss"],
    "className": "org.treesitter.TreeSitterScss"
  }
}